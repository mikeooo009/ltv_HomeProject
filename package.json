{
  "name": "admin-dashboard",
  "version": "1.0.0",
  "description": "Admin Dashboard for managing temporary phone numbers",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd frontend-app && npm install",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd backend/frontend-app && npm start",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build": "cd backend/frontend-app && npm run build",
    "test": "cd backend/frontend-app && npm test"
  },
  "keywords": [
    "angular",
    "nodejs",
    "express",
    "admin-dashboard",
    "phone-numbers"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
